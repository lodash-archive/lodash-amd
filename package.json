{
  "name": "lodash-amd",
  "version": "2.4.1",
  "description": "A collection of Lo-Dash methods as AMD modules generated by lodash-cli.",
  "homepage": "http://lodash.com/custom-builds",
  "license": "MIT",
  "main": "modern/main.js",
  "keywords": ["amd", "browser", "client", "customize", "functional", "lodash", "modules", "util"],
  "author": "John-David Dalton <john.david.dalton@gmail.com> (http://allyoucanleet.com/)",
  "contributors": [
    "John-David Dalton <john.david.dalton@gmail.com> (http://allyoucanleet.com/)",
    "Blaine Bublitz <blaine@iceddev.com> (http://www.iceddev.com/)",
    "Kit Cambridge <github@kitcambridge.be> (http://kitcambridge.be/)",
    "Mathias Bynens <mathias@qiwi.be> (http://mathiasbynens.be/)"
  ],
  "bugs": "https://github.com/lodash/lodash-cli/issues",
  "repository": "lodash/lodash-amd",
  "scripts": { "test": "echo \"See https://travis-ci.org/lodash/lodash-amd for testing details.\"" },
  "volo": {
    "type": "directory",
    "ignore": [
      ".*",
      "*.log",
      "*.md",
      "bower.json",
      "coverage",
      "node_modules"
    ]
  },
  "browserify": {
    "transform": "deamdify"
  }
}
